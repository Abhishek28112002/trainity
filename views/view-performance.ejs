    function Throttling(f, delay) {
      let flag = true;
      return function() {
        let context = this;
        let args = arguments;
        if (flag) {
          f.apply(context, args);
          flag = false;
          setTimeout(() => {
            flag = true;
          }, delay)
        }
      }
    }

