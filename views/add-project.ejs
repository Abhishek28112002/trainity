    function getKPIUnit(krIndex, kpIndex) {
      const kpi = krsinfo[krIndex].subkrs[kpIndex];
      if (kpi.type === "rupees") return "₹";
      else if (kpi.type === "percentages") return "%";
      else if (kpi.type === "numeric") return "";
      else if (kpi.type === "minutes") return "Min";
      else if (kpi.type === "Seconds") return "S";
      else return kpi.type;
    }

