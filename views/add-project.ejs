    function formatDate(dateString) 
    {
      // Split the date string into month, day, and year
      var parts = dateString.split('/');
      var month = parseInt(parts[0]);
      var day = parseInt(parts[1]);
      var year = parseInt(parts[2]);

      // Create an array with month names
      var monthNames = [
        'January', 'February', 'March', 'April', 'May', 'June',
        'July', 'August', 'September', 'October', 'November', 'December'
      ];

      // Create a Date object using the provided year, month (zero-based), and day
      var date = new Date(year, month - 1, day);

      // Get the month name from the monthNames array
      var monthName = monthNames[date.getMonth()];

      // Format the date as "day monthName year"
      var formattedDate = date.getDate() + ' ' + monthName;

      return formattedDate;

    }

    

