    function progressbar(start, mid, end, progressbar, valuefield = "",krno=krsno,kpno=kpino) {
      let s = document.getElementById(start).value;
      let m = document.getElementById(mid);
      let e = document.getElementById(end).value;
      let value = document.getElementById(progressbar).value;

      m.value = (parseInt(s) + (parseInt(e) - parseInt(s)) * (value / 100)).toFixed(2);
      krsinfo[krno].subkrs[kpno].start = s;
      krsinfo[krno].subkrs[kpno].end = e;
      krsinfo[krno].subkrs[kpno].progressvalue = value;
      krsinfo[krno].subkrs[kpno].mid = m.value;

      const valuefields = document.getElementById(valuefield);
      if (valuefields) valuefields.innerHTML = `${value} %`;
      updateprogressvalue(krno);

    }
    
    function updateprogressvalue(krno)
    {
      const modalprogress = document.getElementById("modal-krprogress");
  const modalprogressvalue = document.getElementById("modal-krprogressvalue");
  const progress=document.getElementById("progressbar-"+krno);
      const progressvalue=document.getElementById("progress-"+krno);
      let subkrs=krsinfo[krno].subkrs;
      let avg=0;
      for(let i=1;i<subkrs.length;i++)
      {
        avg+= Number(subkrs[i].progressvalue);
      }
   
      avg=(avg/(subkrs.length -1)).toFixed(2);
      modalprogressvalue.innerHTML=`${avg} %`;
      progressvalue.innerHTML=`${avg} %`;
      modalprogress.value=avg
      progress.value=avg;
     krsinfo[krno].progress=avg;

    }

