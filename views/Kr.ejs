    function SaveinitiativesMeasurments() {
  let matrixType = document.getElementById("initiativesMatrixTypeModal").innerText;
  if(matrixType=='Percentage')
      matrixType='%';
      else if(matrixType=='Currency')
      matrixType='';
      else if(matrixType=='Numerical')
     matrixType=''
  let startingMatrixValue = document.getElementById("initiativesStartingMatrixValueModal").value;
  let currentMatrixValue = document.getElementById("initiativesCurrentMatrixValueModal").value;
  let targetMatrixValue = document.getElementById("initiativesTargetMatrixValueModal").value;
  initiatives[subkrindex][initiativesIndex].matrix = { type: matrixType, startingValue: startingMatrixValue, currentValue: currentMatrixValue, targetValue: targetMatrixValue }
  let matrixCV = document.getElementById('ProgressBarCV' + subkrindex +'-'+initiativesIndex);
  let matrixIV = document.getElementById('ProgressBarIV' + subkrindex+'-'+initiativesIndex);
  let matrixTV = document.getElementById('ProgressBarTV' + subkrindex+'-'+initiativesIndex);
  let matrixBar = document.getElementById('ProgressBar' + subkrindex+'-'+initiativesIndex);
  matrixCV.innerHTML = currentMatrixValue + matrixType;
  matrixIV.innerHTML = startingMatrixValue + matrixType;
  matrixTV.innerHTML = targetMatrixValue + matrixType;
  let Percentage_ = (currentMatrixValue - startingMatrixValue) / (targetMatrixValue - startingMatrixValue) * 100;
  
  Percentage_ = Math.min(100, Math.max(0, Percentage_));
  
  matrixBar.style.width = Percentage_ + '%';
  matrixCV.style.marginLeft = Percentage_ + '%';
  CreateinitiativesDataApi(subkrindex,initiativesIndex)
}
    
