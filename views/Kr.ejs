  var end = krs.timePeriod?.endDate?.length > 0 ? moment(krs.timePeriod.endDate) : moment();


      function cb(start, end) {
        $("#reportrange span").html(
          start.format("MMM D") + " - " + end.format("MMM D, YYYY")
        );
        krs.timePeriod.startDate = start.format("MMMM D, YYYY");
        krs.timePeriod.endDate = end.format("MMMM D, YYYY");
        SavekrsDataApi()
      }

      $("#reportrange").daterangepicker(
        {
          startDate: start,
          endDate: end,
          ranges: {
            "This Year": [moment().startOf("year"), moment().endOf("year")],
            "Next 6 Months": [moment(), moment().add(6, "months")],
            "Next 3 Months": [moment(), moment().add(3, "months")],
            "Last 30 Days": [moment().subtract(29, "days"), moment()],
            "This Month": [moment().startOf("month"), moment().endOf("month")],
            "Last Month": [
              moment().subtract(1, "month").startOf("month"),
              moment().subtract(1, "month").endOf("month"),
            ],
          },
        },
        cb
      );

      cb(start, end);
    }
