    async function CreatesubKeyResultDataApi(i,j) {
      try {
        let res = await fetch('/create/subkeyresult', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify(subkrs[i][j])
        })
        res=await res.json();
        subkrs[i][j]._id=res._id;
      }
      catch (error) {
        console.log(error);
      }
    }
